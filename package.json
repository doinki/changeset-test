{
  "name": "changeset-testtest",
  "version": "0.0.5",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/doinki/changeset-test.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
